<Page x:Class="GreeniumPrototype.Feedback"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GreeniumPrototype"
      mc:Ignorable="d" 
      d:DesignHeight="527" d:DesignWidth="200"
      Title="Feedback">

    <Grid Background="White">
        <TextBox x:Name="CommentTextbox" HorizontalAlignment="Left" Height="460" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="200" Margin="0,20,0,0"/>
        <Button x:Name="SendFeedbackBtn" Content="Envoyer" HorizontalAlignment="Left" Margin="3,480,0,0" VerticalAlignment="Top" Width="95" Height="37" BorderBrush="{x:Null}" Background="{x:Null}" BorderThickness="0"/>
        <Button x:Name="CancelFeedbackBtn" Content="Annuler" HorizontalAlignment="Left" Margin="102,480,0,0" VerticalAlignment="Top" Width="95" Height="37" Background="{x:Null}" BorderThickness="0"/>

    </Grid>
</Page>
