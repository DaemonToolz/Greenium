<Page x:Class="GreeniumPrototype.Emails"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GreeniumPrototype"
      mc:Ignorable="d" 
      d:DesignHeight="527" d:DesignWidth="200"
      Title="Emails">

    <Grid Background="White">
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="527" VerticalAlignment="Top" Width="200">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
